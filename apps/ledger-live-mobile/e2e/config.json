{
  "globalSetup": "./global-setup.js",
  "globalTeardown": "./global-teardown.js",
  "setupFilesAfterEnv": ["./setup.js"],
  "maxWorkers": 1,
  "testEnvironment": "./environment",
  "testRunner": "jest-circus/runner",
  "testTimeout": 120000,
  "testRegex": "\\.spec\\.ts$",
  "transform": {
    "\\.(j|t)sx?$": "ts-jest"
