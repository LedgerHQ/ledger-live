  "productName": "Ledger Live",
  "description": "Ledger Live - Desktop",
  "author": "Ledger Live Team <team-live@ledger.fr>",
  "repository": {
    "type": "git",
    "url": "https://github.com/LedgerHQ/ledger-live.git"
