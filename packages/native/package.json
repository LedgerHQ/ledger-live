{
  "name": "@ledgerhq/native-ui",
  "version": "0.2.0",
  "private": true,
  "description": "Ledger Live - Desktop UI",
  "repository": "https://github.com/LedgerHQ/ui",
  "license": "MIT",
  "author": "Ledger Live Team <team-live@ledger.fr>",
  "main": "index.js",
  "files": [
    "components",
    "assets",
    "styles",
    "helpers",
    "icons",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "android": "expo start --android",
    "build": "tsc -p tsconfig.prod.json",
    "build:storybook": "build-storybook -o web-build",
    "build:icons": "node -e \"process.env['VERCEL_BUILD'] ? process.exit(1) : process.exit(0)\" || (cd ../icons && npm run build)",
    "build:web": "npm run build:icons && npm run build:storybook",
    "clean": "rimraf lib",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --cache",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix --cache",
    "prerelease": "node scripts/prepareRelease.js",
    "release": "yarn publish ./lib --access public",
    "start": "expo start",
    "storybook": "start-storybook -p 6006",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "web": "expo start --web"
  },
  "dependencies": {
    "@ledgerhq/icons-ui": "^0.1.0",
    "@ledgerhq/ui-shared": "^0.1.1",
    "@types/color": "^3.0.2",
