{
  "baseBranch": "origin/develop",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "lib/**", "lib-es/**", "build/**"]
    },
    "build-ci": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "lib/**", "lib-es/**", "build/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "start": {
      "cache": false,
      "dependsOn": ["^build"]
    }
  }
}
