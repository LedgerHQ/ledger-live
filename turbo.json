{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/develop",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "lib/**", "lib-es/**", "build/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "typecheck": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["lint", "typecheck", "build"],
      "outputs": []
    },
    "start": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "build-ci": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "nightly": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    "pre-build": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    "release": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    "watch": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "doc": {
      "cache": false
    },
    "@ledgerhq/icons-ui#build": {
      "dependsOn": ["^build"],
      "outputs": ["react/**", "native/**", "index.js"]
    },
    "@ledgerhq/live-common#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "lib/**",
        "src/generated/**",
        "src/data/flags/react",
        "src/data/flags/reactNative"
      ]
    },
    "live-mobile#lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "ledger-live-desktop#lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "ledger-live-desktop#test": {
      "dependsOn": ["ledger-live-desktop#build:testing"],
      "outputs": []
    },
    "ledger-live-desktop#build:testing": {
      "cache": false,
      "dependsOn": ["^build"]
    }
  }
}
