{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/develop",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "lib/**",
        "lib-es/**",
        "build/**"
      ]
    },
    "build-ci": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "typecheck": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "lint",
        "typecheck",
        "build"
      ],
      "outputs": []
    },
    "live-mobile#lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "live-desktop#lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "start": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    }
  }
}