{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "lib/**",
        "lib-es/**",
        "build/**"
      ]
    },
    "start": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    }
  }
}