{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/develop",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "lib/**",
        "lib-es/**",
        "build/**",
        "react/**",
        "native/**",
        "index.js"
      ]
    },
    "nightly": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    // "icons-ui#build": {
    //   "outputs": [
    //     "react/**",
    //     "native/**",
    //     "index.js"
    //   ]
    // },
    "build-ci": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "live-mobile#lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "live-desktop#lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "typecheck": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["lint", "typecheck", "build"],
      "outputs": []
    },
    "start": {
      "cache": false,
      "dependsOn": ["^build"]
    }
  }
}
