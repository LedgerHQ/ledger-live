{
  "name": "ledger-live-mobile-e2e-tests",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "global": "npx ts-node jest.globalSetup.ts",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "jest --config jest.config.ts",
    "e2e:prepare:ios": "detox clean-framework-cache && detox build-framework-cache",
    "e2e:build:ios": "pnpm --filter live-mobile run e2e:build --configuration ios.sim.debug",
    "e2e:build:android": "pnpm --filter live-mobile run e2e:build --configuration android.emu.release",
    "e2e:build:staging": "pnpm --filter live-mobile run e2e:build --configuration ios.sim.staging",
    "e2e:test": "pnpm detox test --configuration ios.sim.debug",
    "e2e:test:ios": "pnpm e2e:test --configuration ios.sim.debug",
    "e2e:test:android": "pnpm e2e:test --configuration android.emu.debug",
    "e2e:test:parallel": "pnpm e2e:test --workers 2",
    "e2e:test:skip-build": "pnpm e2e:test --configuration ios.sim.debug --no-build",
    "e2e:test:specific": "pnpm e2e:test --configuration ios.sim.debug specs/addAccount/addAccountTRX.spec.ts specs/addAccount/addAccountSOL.spec.ts specs/swap/swapBTC_NATIVE_SEGWIT_SOL.spec.ts",
    "e2e:test:specific:ios": "pnpm e2e:test:specific --configuration ios.sim.debug",
    "e2e:test:specific:android": "pnpm e2e:test:specific --configuration android.emu.debug",
    "e2e:run:ios": "pnpm e2e:build:ios && pnpm e2e:test:ios",
    "e2e:run:android": "pnpm e2e:build:android && pnpm e2e:test:android",
    "e2e:run:specific:ios": "pnpm e2e:build:ios && pnpm e2e:test:specific:ios",
    "e2e:run:specific:android": "pnpm e2e:build:android && pnpm e2e:test:specific:android",
    "e2e:clean": "detox clean-framework-cache && rm -rf artifacts allure-report",
    "allure:generate": "allure generate artifacts --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve artifacts"
  },
  "dependencies": {
    "@ledgerhq/logs": "workspace:^",
    "invariant": "2.2.4",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@jest/reporters": "29.7.0",
    "@jest/types": "^29.6.3",
    "@ledgerhq/devices": "workspace:*",
    "@ledgerhq/hw-transport": "workspace:^",
    "@ledgerhq/hw-transport-node-speculos-http": "workspace:^",
    "@ledgerhq/ledger-key-ring-protocol": "workspace:^",
    "@ledgerhq/live-common": "workspace:^",
    "@ledgerhq/live-env": "workspace:^",
    "@ledgerhq/live-wallet": "workspace:^",
    "@ledgerhq/test-utils": "workspace:^",
    "@ledgerhq/types-cryptoassets": "workspace:^",
    "@ledgerhq/types-devices": "workspace:^",
    "@ledgerhq/types-live": "workspace:^",
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/invariant": "2.2.37",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.0",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "allure-commandline": "2.28.0",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "bignumber.js": "9.1.2",
    "body-parser": "1.20.3",
    "chalk": "^5.4.1",
    "cors": "2.8.5",
    "detox": "20.34.4",
    "detox-allure2-adapter": "1.0.0-alpha.12",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "expect": "27.5.1",
    "express": "4.19.2",
    "glob": "^11.0.1",
    "jest": "29.7.0",
    "jest-allure2-reporter": "2.0.0",
    "jest-docblock": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-metadata": "1.5.3",
    "lodash": "4.17.21",
    "metro-react-native-babel-preset": "0.77.0",
    "perf_hooks": "^0.0.1",
    "react-native-config": "1.5.3",
    "react-native-launch-arguments": "4.0.2",
    "react-native-reanimated": "3.16.1",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "ws": "7.5.10"
  }
}