{
  "name": "@ledgerhq/client-ids",
  "version": "0.5.1",
  "description": "Ledger Live client IDs management library",
  "keywords": [
    "Ledger"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LedgerHQ/ledger-live.git"
  },
  "bugs": {
    "url": "https://github.com/LedgerHQ/ledger-live/issues"
  },
  "homepage": "https://github.com/LedgerHQ/ledger-live/tree/develop/libs/client-ids",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "lib-es/index.js",
  "types": "lib/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf lib lib-es",
    "build": "tsc && tsc -m esnext --moduleResolution bundler --outDir lib-es",
    "prewatch": "pnpm build",
    "watch": "tsc --watch",
    "watch:es": "tsc --watch -m esnext --moduleResolution bundler --outDir lib-es",
    "lint": "node scripts/check-export-rules.mjs && eslint ./src --no-error-on-unmatched-pattern --ext .ts,.tsx --cache",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@ledgerhq/live-env": "workspace:^",
    "@reduxjs/toolkit": "catalog:",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "catalog:",
    "@types/node": "^22.10.10",
    "@types/react": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "jest": "catalog:",
    "react-redux": "catalog:",
    "react-dom": "catalog:",
    "react": "catalog:",
    "rimraf": "^4.4.1",
    "@swc/jest": "catalog:",
    "@swc/core": "catalog:"
  },
  "typesVersions": {
    "*": {
      "*.json": [
        "*.json"
      ],
      "*": [
        "lib/*"
      ],
      "lib/*": [
        "lib/*"
      ],
      "lib-es/*": [
        "lib-es/*"
      ]
    }
  },
  "exports": {
    "./ids": {
      "types": "./lib/ids/index.d.ts",
      "require": "./lib/ids/index.js",
      "default": "./lib-es/ids/index.js"
    },
    "./store": {
      "types": "./lib/store/index.d.ts",
      "require": "./lib/store/index.js",
      "default": "./lib-es/store/index.js"
    },
    "./api": {
      "types": "./lib/api/index.d.ts",
      "require": "./lib/api/index.js",
      "default": "./lib-es/api/index.js"
    },
    "./lib/*": "./lib/*.js",
    "./lib/*.js": "./lib/*.js",
    "./lib-es/*": "./lib-es/*.js",
    "./lib-es/*.js": "./lib-es/*.js",
    "./package.json": "./package.json"
  }
}
