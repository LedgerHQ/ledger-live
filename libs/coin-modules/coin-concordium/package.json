{
  "name": "@ledgerhq/coin-concordium",
  "version": "0.3.0",
  "description": "Concordium coin integration",
  "keywords": [
    "Ledger",
    "LedgerWallet",
    "concordium",
    "Hardware Wallet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LedgerHQ/ledger-live.git"
  },
  "bugs": {
    "url": "https://github.com/LedgerHQ/ledger-live/issues"
  },
  "homepage": "https://github.com/LedgerHQ/ledger-live/tree/develop/libs/coin-modules/coin-concordium",
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib/*"
      ],
      "lib-es/*": [
        "lib-es/*"
      ],
      "api": [
        "lib/api/index"
      ],
      "deviceTransactionConfig": [
        "lib/bridge/deviceTransactionConfig"
      ],
      "logic": [
        "lib/logic/index"
      ],
      "specs": [
        "lib/test/bot-specs"
      ],
      "test/concordiumTestUtils": [
        "lib/test/concordiumTestUtils"
      ],
      "transaction": [
        "lib/bridge/transaction"
      ],
      "types": [
        "lib/types/index"
      ],
      "*": [
        "lib/*",
        "lib/bridge/*",
        "lib/logic/*",
        "lib/signer/*",
        "lib/test/*",
        "lib/types/*"
      ]
    }
  },
  "exports": {
    "./lib/*": "./lib/*.js",
    "./lib-es/*": "./lib-es/*.js",
    "./api": {
      "require": "./lib/coin-modules/coin-concordium/src/api/index.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/api/index.js"
    },
    "./bridge/index": {
      "require": "./lib/coin-modules/coin-concordium/src/bridge/index.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/bridge/index.js"
    },
    "./deviceTransactionConfig": {
      "require": "./lib/coin-modules/coin-concordium/src/bridge/deviceTransactionConfig.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/bridge/deviceTransactionConfig.js"
    },
    "./logic": {
      "require": "./lib/coin-modules/coin-concordium/src/common-logic/index.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/common-logic/index.js"
    },
    "./signer": {
      "require": "./lib/coin-modules/coin-concordium/src/signer/index.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/signer/index.js"
    },
    "./specs": {
      "require": "./lib/coin-modules/coin-concordium/src/test/bot-specs.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/test/bot-specs.js"
    },
    "./test/concordiumTestUtils": {
      "require": "./lib/coin-modules/coin-concordium/src/test/concordiumTestUtils.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/test/concordiumTestUtils.js"
    },
    "./transaction": {
      "require": "./lib/coin-modules/coin-concordium/src/bridge/transaction.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/bridge/transaction.js"
    },
    "./types": {
      "require": "./lib/coin-modules/coin-concordium/src/types/index.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/types/index.js"
    },
    "./network/onboard": {
      "require": "./lib/coin-modules/coin-concordium/src/network/onboard.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/network/onboard.js"
    },
    "./network/utils": {
      "require": "./lib/coin-modules/coin-concordium/src/network/utils.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/network/utils.js"
    },
    "./network/walletConnect": {
      "require": "./lib/coin-modules/coin-concordium/src/network/walletConnect.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/network/walletConnect.js"
    },
    "./*": {
      "require": "./lib/coin-modules/coin-concordium/src/*.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/*.js"
    },
    ".": {
      "require": "./lib/coin-modules/coin-concordium/src/index.js",
      "default": "./lib-es/coin-modules/coin-concordium/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@ledgerhq/concordium-sdk-adapter": "workspace:*",
    "@ledgerhq/coin-framework": "workspace:^",
    "@ledgerhq/cryptoassets": "workspace:^",
    "@ledgerhq/devices": "workspace:*",
    "@ledgerhq/errors": "workspace:^",
    "@ledgerhq/hw-app-concordium": "workspace:*",
    "@ledgerhq/hw-transport": "workspace:*",
    "@ledgerhq/live-env": "workspace:^",
    "@ledgerhq/live-network": "workspace:^",
    "@ledgerhq/types-live": "workspace:^",
    "@walletconnect/sign-client": "2.23.0",
    "@walletconnect/types": "2.23.0",
    "bignumber.js": "^9.1.2",
    "invariant": "^2.2.4",
    "json-bigint": "^1.0.0",
    "rxjs": "catalog:"
  },
  "devDependencies": {
    "@ledgerhq/disable-network-setup": "workspace:^",
    "@ledgerhq/types-cryptoassets": "workspace:^",
    "@types/invariant": "^2.2.37",
    "@types/jest": "^29.5.12",
    "@types/json-bigint": "^1.0.4",
    "dotenv": "^16.4.5",
    "expect": "^27.4.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "clean": "rimraf lib lib-es",
    "build": "tsc && tsc -m esnext --moduleResolution bundler --outDir lib-es",
    "coverage": "jest --coverage",
    "prewatch": "pnpm build",
    "watch": "tsc --watch",
    "doc": "documentation readme src/** --section=API --pe ts --re ts --re d.ts",
    "lint": "eslint ./src --no-error-on-unmatched-pattern --ext .ts,.tsx --cache",
    "lint:fix": "pnpm lint --fix",
    "test": "jest",
    "test-integ": "DOTENV_CONFIG_PATH=.env.integ.test jest --config=jest.integ.config.js --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "unimported": "unimported"
  }
}
