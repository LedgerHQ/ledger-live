{
  "name": "@ledgerhq/coin-tester-solana",
  "version": "1.12.0-next.0",
  "description": "Ledger Solana Coin Tester",
  "main": "src/scenarii.test.ts",
  "keywords": [
    "Ledger",
    "LedgerWallet",
    "Solana",
    "Testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LedgerHQ/ledger-live.git"
  },
  "bugs": {
    "url": "https://github.com/LedgerHQ/ledger-live/issues"
  },
  "homepage": "https://github.com/LedgerHQ/ledger-live/tree/develop/libs/coin-tester-modules/coin-tester-solana",
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib/*"
      ],
      "lib-es/*": [
        "lib-es/*"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "exports": {
    "./lib/*": "./lib/*.js",
    "./lib-es/*": "./lib-es/*.js",
    "./*": {
      "require": "./lib/*.js",
      "default": "./lib-es/*.js"
    },
    "./package.json": "./package.json"
  },
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf lib lib-es",
    "build": "tsc && tsc -m esnext --moduleResolution bundler --outDir lib-es",
    "prewatch": "pnpm build",
    "watch": "tsc --watch",
    "watch:es": "tsc --watch -m esnext --moduleResolution bundler --outDir lib-es",
    "lint": "eslint ./src --no-error-on-unmatched-pattern --ext .ts,.tsx --cache",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "start": "DOTENV_CONFIG_PATH=.env pnpm jest --runTestsByPath src/*.test.ts --runInBand",
    "unimported": "unimported"
  },
  "dependencies": {
    "@ledgerhq/coin-framework": "workspace:^",
    "@ledgerhq/coin-tester": "workspace:^",
    "@ledgerhq/coin-solana": "workspace:^",
    "@ledgerhq/cryptoassets": "workspace:^",
    "@ledgerhq/live-env": "workspace:^",
    "@ledgerhq/types-cryptoassets": "workspace:^",
    "@ledgerhq/types-live": "workspace:^",
    "@solana/spl-token": "catalog:",
    "@solana/web3.js": "catalog:",
    "bignumber.js": "^9",
    "bip39": "^3.1.0",
    "docker-compose": "^1",
    "ed25519-hd-key": "^1.3.0",
    "msw": "catalog:"
  },
  "devDependencies": {
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "rimraf": "^5",
    "@swc/jest": "catalog:",
    "@swc/core": "catalog:",
    "ts-node": "^10",
    "dotenv": "^16",
    "typescript": "^5"
  }
}