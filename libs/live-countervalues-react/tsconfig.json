{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "downlevelIteration": true,
    "jsx": "react",
    "lib": ["es2020", "dom"],
    "outDir": "lib",
    "baseUrl": ".",
    "paths": {
      "react": ["../../node_modules/@types/react"],
      "@types/react": ["../../node_modules/@types/react"]
    }
  },
  "include": ["src/**/*"]
}
