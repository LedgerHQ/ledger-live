{
  "extends": "../../../../tsconfig.base",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "jsx": "react",
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": ".",
    "noEmit": true,
    "moduleResolution": "bundler",
    "module": "esnext",
    "target": "esnext",
    "skipLibCheck": true,
    "paths": {
      "react": ["../../../../node_modules/@types/react"],
      "@types/react": ["../../../../node_modules/@types/react"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "react", "native"]
}
