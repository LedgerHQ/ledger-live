{
  "name": "@ledgerhq/native-ui",
  "version": "0.7.3",
  "description": "Ledger Live - Mobile UI",
  "repository": "https://github.com/LedgerHQ/ui",
  "license": "MIT",
  "author": "Ledger Live Team <team-live@ledger.fr>",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "lib/*",
        "lib/*/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./lib/index.js",
    "./": "./lib/",
    "./package.json": "./package.json"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "android": "expo start --android",
    "build": "tsc -p tsconfig.prod.json && node scripts/postBuild",
    "build:storybook": "build-storybook -o web-build",
    "build:icons": "node -e \"process.env['VERCEL_BUILD'] ? process.exit(1) : process.exit(0)\" || (cd ../icons && npm run build)",
    "build:web": "npm run build:icons && npm run build:storybook",
    "watch": "tsc --watch",
    "clean": "rimraf lib",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "lint": "eslint src storybook --ext .ts,.tsx,.js,.jsx --cache",
    "lint:fix": "eslint src storybook --ext .ts,.tsx,.js,.jsx --fix --cache",
    "release": "yarn publish ./lib --access public",
    "expo": "expo start",
    "storybook": "start-storybook -p 6006",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "web": "expo start --web"
  },
  "dependencies": {
    "@ledgerhq/icons-ui": "workspace:^",
    "@ledgerhq/ui-shared": "workspace:^",
    "@types/color": "^3.0.3",
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.65.9",
    "@types/styled-components-react-native": "^5.1.3",
    "@types/styled-system": "^5.1.13",
    "color": "^3.1.3",
    "moment": "^2.29.1",
    "react-native-modal": "^13.0.0",
    "rn-range-slider": "^2.1.1",
    "styled-system": "^5.1.5",
    "victory-native": "^35.5.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-native": ">=0.64.0",
    "react-native-reanimated": "^2.4.1",
    "react-native-svg": "^12.1.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.0",
    "@babel/preset-env": "^7.15.8",
    "@babel/runtime": "^7.17.2",
    "@babel/generator": "^7.17.2",
    "@emotion/core": "^11.0.0",
    "@expo/webpack-config": "^0.16.18",
    "@react-native-community/eslint-config": "^3.0.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addons": "^6.4.19",
    "@storybook/api": "^6.4.19",
    "@storybook/components": "^6.4.19",
    "@storybook/core-events": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/react-native": "^5.3.25",
    "@storybook/theming": "^6.4.19",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "^9.0.2",
    "camelcase": "^6.2.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "expo": "~44.0.0",
    "expo-asset": "~8.4.6",
    "expo-file-system": "~13.1.3",
    "expo-cli": "^5",
    "expo-font": "^10.0.5",
    "expo-status-bar": "~1.2.0",
    "expo-constants": "~13.0.2",
    "expo-modules-core": "0.6.5",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "metro": "^0.67.0",
    "metro-extra-config": "workspace:*",
    "metro-config": "^0.67.0",
    "metro-transform-worker": "^0.67.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "metro-minify-uglify": "^0.67.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "~2.3.1",
    "react-native-svg": "^12.1.1",
    "react-native-web": "~0.17.5",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4",
    "webpack": "^4.46.0"
  }
}