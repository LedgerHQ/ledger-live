{
  "name": "ledgerlivemobile",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "postinstall": "./scripts/post.sh",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "internal": "FASTLANE_XCODEBUILD_SETTINGS_TIMEOUT=30 FASTLANE_XCODEBUILD_SETTINGS_RETRIES=5 bundle exec fastlane beta release:true",
    "preinternal": "bundle install",
    "beta": "FASTLANE_XCODEBUILD_SETTINGS_TIMEOUT=30 FASTLANE_XCODEBUILD_SETTINGS_RETRIES=5 bundle exec fastlane beta",
    "prebeta": "bundle install",
    "android": "react-native run-android --appIdSuffix=debug",
    "android:apk": "bundle exec fastlane android apk",
    "android:beta": "bundle exec fastlane android clean_beta",
    "preandroid:beta": "bundle install",
    "android:upload": "bundle exec fastlane android upload",
    "android:internal": "bundle exec fastlane android beta",
    "preandroid:internal": "bundle install",
    "android:clean": "$ANDROID_HOME/platform-tools/adb shell pm clear com.ledger.live",
    "android:import": "./scripts/android-import.sh",
    "android:staging": "cd android && ./gradlew assembleStagingRelease",
    "android:mock": "cd android && ENVFILE=.env.mock ./gradlew assembleStagingRelease",
    "android:release": "./scripts/android-release.sh",
    "android:install": "./scripts/install-and-run-apk.sh",
    "staging-android": "yarn android:staging && yarn android:install",
    "mock-android": "yarn android:mock && yarn android:install",
    "ios:ipa": "bundle exec fastlane ios build_ipa --env production",
    "ios:ipadev": "bundle exec fastlane ios build_ipa development:true --env production",
    "ios:beta": "bundle exec fastlane ios clean_beta --env production",
    "preios:beta": "bundle install",
    "ios:upload": "bundle exec fastlane ios upload",
    "ios:internal": "bundle exec fastlane ios beta --env production",
    "preios:internal": "bundle install",
    "ios:staging": "ENVFILE=.env.staging react-native run-ios --configuration Staging",
    "prettier": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src",
    "flow": "flow",
    "test": "yarn check --integrity && ./scripts/check-no-dups.sh && yarn lint --quiet",
    "sync-flowtyped": "NODE_OPTIONS=--max_old_space_size=10000 flow-typed install -s -o && rm flow-typed/npm/axios_*",
    "sync-locales": "./scripts/sync-locales.sh",
    "debugSVG": "sh scripts/debugsvg.sh | node_modules/.bin/prettier --parser flow > src/screens/DebugSVG.js",
    "pod": "cd ios && bundle exec pod install --repo-update",
    "hack-for-ios-simulator": "cd node_modules/@ledgerhq/react-native-ledger-core/ios/Frameworks && mv universal real_universal && cp -R x86 universal"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
